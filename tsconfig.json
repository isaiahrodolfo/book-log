{
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./*",
        "./components",
      ],
      "@useAuth": ["providers/AuthProvider.tsx"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "api/books.js",
    "app/(tabs)/books.jsx"
  ],
  "extends": "expo/tsconfig.base"
}
